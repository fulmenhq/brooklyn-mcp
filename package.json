{
  "name": "fulmen-mcp-forge-brooklyn",
  "version": "1.1.6",
  "description": "Enterprise-ready MCP server for browser automation with multi-team support",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "module": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun scripts/extract-help-text.ts && mkdir -p dist && bun run version:embed && bun build src/cli/brooklyn.ts --outfile dist/brooklyn --target node --external playwright --external @playwright/test --external electron && chmod +x dist/brooklyn",
    "setup": "bun install && bunx playwright install",
    "start": "bun run src/cli/brooklyn.ts mcp start",
    "server:start": "bun scripts/server-management.ts start",
    "server:stop": "bun scripts/server-management.ts stop",
    "server:restart": "bun scripts/server-management.ts restart",
    "server:status": "bun scripts/server-management.ts status",
    "server:cleanup": "bun scripts/server-management.ts cleanup",
    "server:logs": "bun scripts/server-management.ts logs",
    "server:logs:recent": "bun scripts/server-management.ts logs --recent",
    "dev:brooklyn:start": "bun scripts/dev-brooklyn.ts start",
    "dev:brooklyn:stop": "bun scripts/dev-brooklyn.ts stop",
    "dev:brooklyn:restart": "bun scripts/dev-brooklyn.ts restart",
    "dev:brooklyn:status": "bun scripts/dev-brooklyn.ts status",
    "dev:brooklyn:logs": "bun scripts/dev-brooklyn.ts logs",
    "dev:brooklyn:cleanup": "bun scripts/dev-brooklyn.ts cleanup",
    "dev:brooklyn:pipes": "bun scripts/dev-brooklyn.ts pipes",
    "dev:brooklyn:test": "bun scripts/dev-brooklyn.ts test",
    "dev:test:basic": "bun scripts/dev-test.ts basic",
    "dev:test:screenshots": "bun scripts/dev-test.ts screenshots",
    "dev:test:security": "bun scripts/dev-test.ts security",
    "dev:test:all": "bun scripts/dev-test.ts all",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "vitest run tests/e2e",
    "lint": "biome check . && prettier --check \"**/*.{json,md,yaml,yml}\"",
    "lint:fix": "biome check --write . && prettier --write \"**/*.{json,md,yaml,yml}\"",
    "format:code": "biome format --write .",
    "format:docs": "prettier --write \"**/*.{json,md,yaml,yml,html}\"",
    "format": "bun run format:code && bun run format:docs",
    "typecheck": "tsc --noEmit",
    "check-all": "bun run format && bun run typecheck && bun run lint && bun run test",
    "check:file": "bun scripts/check-file.ts",
    "check:file:fix": "bun scripts/check-file.ts --fix",
    "version:get": "bun scripts/version.ts get",
    "version:set": "bun scripts/version.ts set",
    "version:bump:patch": "bun scripts/version.ts bump patch",
    "version:bump:minor": "bun scripts/version.ts bump minor",
    "version:bump:major": "bun scripts/version.ts bump major",
    "version:sync": "bun scripts/version.ts sync",
    "version:embed": "bun scripts/embed-version.ts",
    "check:versions": "bun scripts/check-versions.ts",
    "check:versions:fix": "bun scripts/check-versions.ts --fix",
    "// MCP Development Mode (Architecture Committee approved - internal use only)": "",
    "mcp-dev:start": "bun src/cli/brooklyn.ts mcp dev-start",
    "mcp-dev:stop": "bun src/cli/brooklyn.ts mcp dev-stop",
    "mcp-dev:status": "bun src/cli/brooklyn.ts mcp dev-status",
    "mcp-dev:restart": "bun src/cli/brooklyn.ts mcp dev-restart",
    "mcp-dev:cleanup": "bun src/cli/brooklyn.ts mcp dev-cleanup",
    "mcp-dev:test": "bun test --filter=mcp-dev",
    "clean": "rm -rf dist",
    "prepare": "bun run build",
    "bootstrap": "bun scripts/bootstrap-brooklyn.ts",
    "bootstrap:install": "bun scripts/bootstrap-brooklyn.ts install",
    "bootstrap:remove": "bun scripts/bootstrap-brooklyn.ts remove",
    "install": "bun run build && bun scripts/install-cli.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "playwright": "^1.40.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bun": "latest",
    "@types/node": "latest",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "dotenv": "^16.0.0",
    "inquirer": "^9.0.0",
    "@types/inquirer": "^9.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "mcp",
    "browser-automation",
    "playwright",
    "typescript",
    "bun",
    "fulmen",
    "enterprise"
  ],
  "author": "3leaps Fulmen Team",
  "license": "MIT"
}
